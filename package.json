{
  "name": "react-native-store-badges",
  "version": "1.0.2",
  "description": "A React Native library for rendering App Store and Google Play Store badges.",
  "scripts": {
    "prepack": "bob build"
  },
  "keywords": [
    "react-native",
    "component",
    "app store",
    "google play",
    "badges"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/usekeyhole/react-native-store-badges.git"
  },
  "author": "Keyhole",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/usekeyhole/react-native-store-badges/issues"
  },
  "homepage": "https://github.com/usekeyhole/react-native-store-badges#readme",
  "devDependencies": {
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "eslint-import-resolver-babel-module": "^5.3.2",
    "eslint-plugin-import": "^2.28.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-native": "^0.72.4",
    "react-native-builder-bob": "^0.21.3",
    "react-native-web": "^0.19.8",
    "typescript": "^5.2.2"
  },
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "main": "lib/commonjs/index.js",
  "react-native": "src/index.tsx",
  "module": "lib/module/index.js",
  "types": "lib/typescript/index.d.ts",
  "source": "src/index",
  "files": [
    "src",
    "lib"
  ],
  "react-native-builder-bob": {
    "source": "src",
    "output": "lib",
    "targets": [
      "commonjs",
      "module",
      [
        "typescript",
        {
          "project": "tsconfig.build.json"
        }
      ]
    ]
  },
  "eslintIgnore": [
    "node_modules/",
    "lib/"
  ],
  "dependencies": {
    "react-native-svg": "^13.13.0",
    "react-svg": "^16.1.22"
  }
}
